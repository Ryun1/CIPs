    "eraAdded": "Byron",
    "eraRemoved": "-"
  },
  {
    "commonName": "Treasury Rate",
    "specName": "tau",
    "cliName": "treasuryCut",
    "dbSyncName": "treasury_growth_rate",
    "constitutionName": "",
